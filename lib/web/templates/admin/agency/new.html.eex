<div class="content-header">
  <div class="container-fluid">
    <div class="row mb-2">
      <div class="col-sm-6">
        <h1 class="m-0 text-dark">
    New Agency 
        </h1>
      </div>

      <div class="col-sm-6">
        <ol class="breadcrumb float-sm-right">
          <li class="breadcrumb-item">
            <%= link(to: Routes.admin_dashboard_path(@conn, :index)) do %>
              <i class="fa fa-dashboard"></i> Home
            <% end %>
          </li>
          <li class="breadcrumb-item"><%= link("Agencies", to: Routes.admin_agency_path(@conn, :index)) %></li>
          <li class="breadcrumb-item active">New</li>
        </ol>
      </div>
    </div>
  </div>
</div>

<section class="content">
  <div class="container-fluid">
    <div class="row">
      <div class="col-md-12">
        <div class="card">
          <%= render Web.Admin.AgencyView, "_form.html", changeset: @changeset, data: @changeset.data, path: Routes.admin_agency_path(@conn, :create) %>
        </div>
      </div>
    </div>
  </div>
</section>
